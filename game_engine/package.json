{
  "name": "kosa-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node_modules/jest/bin/jest.js",
    "watch": "node_modules/jest/bin/jest.js --watch",
    "fix": "node_modules/prettier/bin-prettier.js --print-width=120 --trailing-comma=all --tab-width=4 --arrow-parens always --write '{src,tests}/**/*.ts'",
    "lint": "node_modules/tslint/bin/tslint --fix {src,test}/**/*.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "prettier": "^1.11.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/cucumber": "^3.2.2",
    "@types/jest": "^21.1.10",
    "@types/ramda": "^0.25.20",
    "cucumber": "^3.2.1",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
